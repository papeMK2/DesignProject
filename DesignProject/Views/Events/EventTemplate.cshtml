@{
    //データベースから引っ張る内容で中身書き換え
    ViewBag.Title = "Event名";
    ViewBag.Date = "20/12/2013";
    ViewBag.Address = "NNCT";
    ViewBag.Num = "30";
    ViewBag.Admin = "hoge";
}


<div class="container">
    <div class="jumbotron">
        <h2>@ViewBag.Title</h2>
        <button type="button" class="btn btn-primary">@Html.ActionLink("参加", "editMyProfile", "Profile", new { @style = "color:#ffffff" })</button>
        <hr style="border-top-color:#000000" />
        <div class="row">
            <div class="col-md-6">
                <div class="row">
                    <div class="col-md-6">
                        <h4>日時 :</h4><br />
                        <h4>開催場所 :</h4><br />
                        <h4>参加人数 :</h4><br />
                        <h4>主催 : </h4>
                    </div>
                    <div class="col-md-6">
                        <h4>@ViewBag.Date</h4><br />
                        <h4>@ViewBag.Address</h4><br />
                        <h4>@ViewBag.Num</h4><br />
                        <h4>@ViewBag.Admin</h4><br />
                        <h4></h4>
                    </div>
                </div>
                <hr style="border-top-color:#000000" />
                <p>
                    イベント説明
                </p>
            </div>
            <div class="col-md-4">
                <h2>地図</h2>
            </div>
            <div class="col-md-2">
                <h2>その他</h2>
            </div>
        </div>
    </div>
</div>
