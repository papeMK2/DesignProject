@{
    ViewBag.Title = "editMyProfile";
}

<form role="form">

</form>
<div class="container">
    <div class="jumbotron">
        <h1>EditMyProfile</h1>
        <button type="button" class="btn btn-primary" >完了</button>
        <button type="button" class="btn btn-primary">キャンセル</button>
        <hr style="border-top-color:#000000" />
        <form role="form">
            <div class="form-group">
                <label for="profileImage">プロフィール画像</label>
                <input type="file" id="profileImage" />
            </div>
            <div class="form-group">
                <label for="lastname">姓</label>
                <input type="text" class="form-control" id="lastname" placeholder="姓"/>
            </div>
            <div class="form-group">
                <label for="firstname">名</label>
                <input type="text" class="form-control" id="firstname" placeholder="名" />

            </div>

        </form>
    </div>
</div>


